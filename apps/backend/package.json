{
  "name": "backend",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev:run": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --dts",
    "start": "tsx src/index.ts",
    "check-types": "tsc --noEmit",
    "dev:types": "tsc --emitDeclarationOnly --watch"
  },
  "alias": {
    "scheduler/tracing": "../../node_modules/scheduler/tracing-profiling"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.3",
    "typescript": "latest",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@repo/auth": "*",
    "@trpc/server": "^11.4.1",
    "better-auth": "^1.2.9",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "minio": "^8.0.5",
    "superjson": "^2.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.51"
  }
}
